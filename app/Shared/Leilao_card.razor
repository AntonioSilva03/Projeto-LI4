@code {
    [Parameter]
    public string weapon { get; set; }

    [Parameter]
    public string skin_name { get; set; }

    [Parameter]
    public string condition { get; set; }
}

<a class="auction-card-container" data-aos="fade-up">
    <img class="skin-picture" src="imgs/test_skin_img.jpeg">

    <div class="weapon-skin">@weapon | @skin_name</div>

    <div class="condition">@condition</div>
</a>